<openerp>
  <data>
<!-- Geoengine View-->
<record  model="ir.ui.view" id="ir_ui_view_urban_bridge_wizard_structure_elem_OSM" >
            <field name="name">urban_bridge.wizard.structure_elem.geo_view</field>
            <field name="arch"><![CDATA[
                <form >
                    <field name="name"/>
                </form>]]></field>
            <field eval="16" name="priority"/>
            <field name="type">geoengine</field>
            <field name="model">urban_bridge.wizard.structure_elem.geo_view</field>
</record>

<!-- Geoengine Base Raster -->
<record model="geoengine.raster.layer" id="geoengine_raster_layer_base_map_district">
            <field name="raster_type">osm</field>
            <field name="name">District OSM</field>
            <field name="view_id" ref="ir_ui_view_base_map_district_OSM"/>
            <field eval="0" name="overlay"/>
</record>

<!-- Geoengine Vector Layer -->

<record  model="geoengine.vector.layer" id="geoengine_vector_layer_base_map_district_plain">
            <field name="geo_field_id" ref="base_map.field_base_map_district_shape"/>
            <field name="name">District</field>
            <field name="view_id" ref="ir_ui_view_base_map_district_OSM"/>
            <field name="geo_repr">basic</field>
            <field name="attribute_field_id" ref="base_map.field_base_map_district_code"/>
            <field name="begin_color">#FF680A</field>
</record>
   
   
   
  <!-- Form View -->
        <record id="view_urban_bridge_wizard_structure_elem_form" model="ir.ui.view">
            <field name="name">Structure Element</field>
            <field name="model">urban_bridge.wizard.structure_elem</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Structure Elements">
                    <field name="elem_id" readonly="True"/>
                    <group col="4" colspan="4">
                        <label string="" colspan="2"/>
                        <button name="elem_create" string="Create" type="object" icon="gtk-ok"/>
                        <button special="cancel" string="_Cancel" icon="gtk-cancel"/>
                    </group>
                </form>
            </field>
        </record>
<!-- Action View -->
        <record id="action_urban_bridge_wizard_structure_elem" model="ir.actions.act_window">
            <field name="name">Elements</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">urban_bridge.wizard.structure_elem</field>
            <field name="src_model">urban_bridge.wizard.structure_elem</field>
            <field name="view_id" ref="view_urban_bridge_wizard_structure_elem_form"/>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
</data>
</openerp>